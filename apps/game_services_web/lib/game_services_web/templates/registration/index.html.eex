<section class="phx-hero">
  <h1>some register page</h1>
</section>
<section>
  <article>
  <%= form_for @conn, Routes.registration_path(@conn, :register), [as: :register], fn f -> %>
    <%= label f, :username  %>
    <%= text_input f, :username %>

    <%= label f, :email %>
    <%= text_input f, :email %>

    <%= label f, :password %>
    <%= text_input f, :password %>

    <%= submit "Register User" %>
   <% end %>
  </article>
</section>
