<section class="phx-hero">
  <h1>some login page</h1>
</section>
<section>
  <article>
  <%= form_for @conn, Routes.login_path(@conn, :login), [as: :login], fn f -> %>
    <%= label f, :username  %>
    <%= text_input f, :username %>

    <%= label f, :password %>
    <%= text_input f, :password %>

    <%= submit "Login User" %>
   <% end %>
  </article>
</section>
